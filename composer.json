{
  "name": "phpformsframework/libs",
  "description": "php FormsFramework libs",
  "license": "LGPL-3.0",
  "type": "library",
  "version": "0.3",
  "authors": [
    {
      "name": "Alessandro Stucchi",
      "email": "wolfgan@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "classmap-authoritative": true,
    "apcu-autoloader": true
  },
  "require": {
    "php": "^7.1.3",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-mysqli": "*",
    "ext-zlib": "*",
    "ext-gd": "*",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-libxml": "*",
    "ext-ftp": "*",
    "phpmailer/phpmailer": "^6.0",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8"
  },
  "autoload": {
    "psr-4": {
      "phpformsframework\\libs\\": "src/"
    },
    "files": [
      "constants.inc.php"
    ]
  },
  "suggest": {
    "mongodb/mongodb": "to use MongoDb",
    "ext-posix": "to extend log info",
    "ext-redis": "to manage cache in memory",
    "twilio/sdk": "to use SMS notifications [composer require twilio/sdk:^5.31.3]"
  }
}